package game.world;

public enum TileType {

	SOLID(64, 64),
	SLOPE_RIGHT_64_00(64, 0),
	SLOPE_LEFT_64_00(0, 64);
	
	
	
	private TileType(int y1, int y2) {
		
	}
	
	public boolean isCubeType(TileType t) {
		return t == SOLID;
	}
	
	public boolean isSlopeRightType(TileType t) {
		return t == SLOPE_RIGHT_64_00;
	}
	
	public boolean isSlopeLeftType(TileType t) {
		return t == SLOPE_LEFT_64_00(0, 64);
	}
}
